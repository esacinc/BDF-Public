# .env

# ChainLit required environmental variables
# CHAINLIT_AUTH_SECRET is a secret string that is used to sign the authentication tokens. 
# You can change it at any time, but it will log out all users. You can easily generate one using "chainlit create-secret" command
# CHAINLIT_AUTH_SECRET=
CHAINLIT_PORT=8000
CHAINLIT_URL=http://localhost:8000


# Having authentication either with email or Google OAuth is needed to preserve chatbot conversations history
# Enable password authentication, we suggest to set to false for local testing
ENABLE_PASSWORD_AUTH=true

# Enable OAuth authentication, we suggest to set to false for local testing
ENABLE_OAUTH=true

# Set your google authentication info here
# OAUTH_GOOGLE_CLIENT_ID=
# OAUTH_GOOGLE_CLIENT_SECRET=

OAUTH_PROMPT=login
OAUTH_GOOGLE_PROMPT=login

# Set your AWS access and secret keys
# AWS_ACCESS_KEY=""
# AWS_SECRET_KEY=""

# Set your AWS region here, for example: us-east-1 
AWS_REGION=""

# Set your AWS Knowledge Base ID here, usually in a format "KB12345678"
PUBLICATIONS_KB_ID=""

# Change default model as needed
DEFAULT_MODEL="us.anthropic.claude-3-5-sonnet-20241022-v2:0"

# Set name of a table that will hold chatbot conversations history
DATA_LAYER_TABLE=""

# API urls
PDC_API=https://proteomic.datacommons.cancer.gov/graphql?query=
